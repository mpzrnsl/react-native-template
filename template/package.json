{
  "name": "ProjectName",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:staging": "react-native run-android --mode=staging",
    "android:staging:install": "cd android && ./gradlew installStaging && cd ..",
    "android:staging:bundle": "cd android && ./gradlew bundleStaging && cd ..",
    "android:staging:assemble": "cd android && ./gradlew assembleStaging && cd ..",
    "android:production": "react-native run-android --mode=release",
    "android:production:install": "cd android && ./gradlew installRelease && cd ..",
    "android:production:bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "android:production:assemble": "cd android && ./gradlew assembleRelease && cd ..",
    "ios": "react-native run-ios",
    "ios:clean": "cd ios && xcodebuild clean && cd ..",
    "ios:staging": "react-native run-ios --scheme \"ProjectName Staging\"",
    "ios:production": "react-native run-ios --scheme \"ProjectName Production\"",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formatjs/intl-getcanonicallocales": "^2.0.5",
    "@formatjs/intl-listformat": "^7.1.7",
    "@formatjs/intl-locale": "^3.0.11",
    "@formatjs/intl-pluralrules": "^5.1.8",
    "@formatjs/intl-relativetimeformat": "^11.1.8",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/drawer": "^6.5.8",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@reduxjs/toolkit": "^1.9.2",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "axios": "^1.3.2",
    "i18next": "^22.4.9",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-i18next": "^12.1.5",
    "react-native": "0.71.2",
    "react-native-config": "^1.5.0",
    "react-native-error-boundary": "^1.2.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-localize": "^2.2.4",
    "react-native-logs": "^5.0.1",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.1.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/test/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
